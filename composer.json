{
    "name": "cct-marketing/kong-php-sdk",
    "description": "Library to execute operations on Kong using PHP",
    "type": "library",
    "require": {
        "php": "~7.1",
        "cct-marketing/collections": "~1.0",
        "guzzlehttp/guzzle": "^6.2",
        "jms/serializer": "^1.10",
        "symfony/yaml": "^4.0",
        "symfony/http-foundation": "^4.0",
        "beberlei/assert": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "CCT\\Kong\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CCT\\Kong\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Vahldick",
            "email": "jorge.dev@outlook.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable"
}
